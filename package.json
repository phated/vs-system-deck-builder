  },
  "homepage": "https://github.com/phated/vs-system-deck-builder#readme",
  "dependencies": {
    "collection-map": "^1.0.0",
    "cxs": "^3.0.4",
    "hammerjs": "^2.0.8",
    "inu": "^3.1.4",
    "level-js": "^2.2.4",
    "levelup": "^1.3.5",
    "object.reduce": "^1.0.0",
    "pull-level": "^2.0.3",
    "pull-notify": "^0.1.1",
